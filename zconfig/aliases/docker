alias d='docker'
alias dm='docker-machine'
alias dc='docker-compose'

alias drm='docker rm'
alias drma='docker rm $(docker ps -qa)'
alias drml='docker rm $(docker ps -ql)'

alias di='docker images'
alias dia='docker images -a'

alias dp='docker ps'
alias dpa='docker ps -a'
alias dpl='docker ps -l'

alias dr='docker run'
alias dri='docker run -ti'

alias de='docker exec'
alias ded='docker exec -d'
alias dei='docker exec -ti'

alias da='docker attach'
alias dl='docker logs'
alias dt='docker top'

alias ds='docker stop'
alias dk='docker kill'

alias dcm='docker commit'

alias drmi='docker rmi'
alias drmia='docker rmi $(docker images -qa)'

alias dma='docker-machine active'
alias dms='docker-machine start'
alias dmst='docker-machine status'
alias dmip='docker-machine ip'

alias dcu='docker-compose up'
alias dcs='docker-compose stop'
