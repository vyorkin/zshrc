alias cr="cabal repl"
alias cg="cabal get"
alias cl="cabal list"
alias ci="cabal info"

alias ccep="cabal configure --enable-profiling"
alias ccdp="cabal configure --disable-profiling"

alias h="hoogle"

alias si="stack install"
alias sb="stack build"
alias se="stack exec --"
alias stf="stack test --fast --haddock-deps"
alias stfw="stack test --fast --haddock-deps --file-watch"
alias sho="stack haddock --open"
alias shs="stack hoogle -- server --local --port=8089"
alias shg="stack hoogle -- generate --local"
alias sgd="stack dot --external | dot -Tpng | feh -"
